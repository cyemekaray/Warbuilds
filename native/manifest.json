{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Warbuilds",
    "version": "1.1.3",
    "minimum-overwolf-version": "0.120.0",
    "author": "Clan AT",
    "icon": "IconMouseOver.png",
    "icon_gray": "IconMouseNormal.png",
    "launcher_icon": "launcher_icon.ico",
    "window_icon": "WindowIcon.png",
    "description": "the Warcraft III build advisor"
  },
  "permissions": ["Hotkeys", "Extensions", "Streaming", "Profile", "GameInfo", "DesktopStreaming"],
  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [21638]
    },
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [21638]
        },
        "start_minimized": true
      }
    ],
    "hotkeys": {
      "sample_app_showhide": {
        "title": "Show/Hide In-Game Window",
        "action-type": "toggle",
        "default": "Ctrl+F"
      },
      "start_pause": {
        "title": "Start/Stop Timer",
        "action-type": "custom",
        "default": "F6"
      },
      "reset": {
        "title": "Reset Timer",
        "action-type": "custom",
        "default": "F9"
      },
      "lockin": {
        "title": "Lock In",
        "action-type": "custom",
        "default": "F5"
      },
      "showhidelockedin": {
        "title": "Show/Hide Locked In Window",
        "action-type": "custom",
        "default": "F7"
      }
    },
    "game_events": [21638],
    "start_window": "background",
    "windows": {
      "background": {
        "file": "windows/background/background.html",
        "background_optimization": false,
        "is_background_page": true
      },
      "desktop": {
        "file": "windows/desktop/desktop.html",
        "native_window": true,
        "transparent": true,
        "resizable": true,
        "size": {
          "width": 1212,
          "height": 699
        },
        "min_size": {
          "width": 1212,
          "height": 699
        },
        "max_size": {
          "width": 1212,
          "height": 699
        },
        "desktop_only": true
      },
      "in_game": {
        "file": "windows/in-game/in-game.html",
        "in_game_only": true,
        "focus_game_takeover": "ReleaseOnHidden",
        "focus_game_takeover_release_hotkey": "sample_app_showhide",
        "transparent": true,
        "resizable": false,
        "size": {
          "width": 1212,
          "height": 699
        },
        "min_size": {
          "width": 1212,
          "height": 699
        },
        "max_size": {
          "width": 1212,
          "height": 699
        }
      },
      "BuildLockedIn": {
        "file": "windows/in-game/BuildLockedIn.html",
        "in_game_only": true,
        "focus_game_takeover": "ReleaseOnHidden",
        "focus_game_takeover_release_hotkey": "sample_app_showhide",
        "transparent": true,
        "resizable": true,
        "disable_rightclick": true,
        "size": {
          "width": 400,
          "height": 500
        }
      },
      "notification": {
        "file": "windows/notification/notification.html",
        "transparent": true,
        "resizable": true,
        "size": {
          "width": 320,
          "height": 260
        },
        "min_size": {
          "width": 320,
          "height": 260
        },
        "max_size": {
          "width": 320,
          "height": 260
        }
      }
    },
    "protocol_override_domains": { "googlesyndication": "http" },
    "externally_connectable": {
      "matches": [
        "http://*.overwolf.com",
        "https://*.overwolf.com",
        "https://overwolf.github.io",
        "https://*.google-analytics.com",
        "http://*.google-analytics.com",
        "https://www.googleapis.com",
        "https://discord.gg/v5cfBTq",
        "https://twitter.com/",
        "https://www.facebook.com",
        "https://www.reddit.com",
        "https://www.youtube.com",
        "https://youtu.be/",
        "https://www.clanat.org/"
      ]
    },
    "force_browser": "overwolf",
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}
